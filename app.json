{
  "expo": {
    "name": "pathways_app",
    "slug": "pathways_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/path_bg.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/path_bg.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.avoidance-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/path_bg.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/path_bg.png"
    },
    "plugins": [
      "expo-router"
         ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
