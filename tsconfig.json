{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "traceResolution": true,
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "types/**/*.d.ts",
    "*.png",
    "**/*.png",
  ]
}